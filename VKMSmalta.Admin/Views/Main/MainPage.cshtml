@using VKMSmalta.Admin.Models.Domain
@{
    Layout = null;
}

<!DOCTYPE html>

<link href='@Url.Content("~/Content/themes/base/all.css")' rel="stylesheet" type="text/css" />
<script src='@Url.Content("~/Scripts/jquery-3.3.1.min.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Scripts/jsTree3/jstree.min.js")' type="text/javascript"></script>

<script>
    $(function () {
        $("#TeamsTree").jstree();
    });
</script>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Преподавательская</title>
</head>
<body>
    <div id="TeamsTree" style="border: 1px solid black">
        <ul>
            @foreach (var team in ViewData["teams"] as IList<Team>)
            {
                <li>@team.Number Взвод</li>
            }
        </ul>
    </div>


    
</body>
</html>