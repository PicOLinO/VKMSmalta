<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:VKMSmalta.Dialogs">
    <Style x:Key="DialogOuterSeparatingRow" TargetType="RowDefinition">
        <Setter Property="Height" Value="15"/>
    </Style>
    <Style x:Key="DialogInnerSeparatingRow" TargetType="RowDefinition">
        <Setter Property="Height" Value="10"/>
    </Style>
    <Style x:Key="DialogOuterSeparatingColumn" TargetType="ColumnDefinition">
        <Setter Property="Width" Value="15"/>
    </Style>
    <Style x:Key="DialogInnerSeparatingColumn" TargetType="ColumnDefinition">
        <Setter Property="Width" Value="10"/>
    </Style>
</ResourceDictionary>